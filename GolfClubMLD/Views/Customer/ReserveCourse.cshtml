@model IEnumerable<GolfClubMLD.Models.CourseTermBO>

@{
    ViewBag.Title = "ReserveCourse";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ReserveCourse</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Term.StartTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Term.EndTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GolfCourse.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dayOfW)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Term.StartTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Term.EndTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GolfCourse.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dayOfW)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
